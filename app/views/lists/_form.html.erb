<div class="row">
<%= form_for @list do |f| %>
  <div class="col l12">
    <%= f.label :name %>
    <%= f.text_field :name, class: "input-field" %>
  </div>
  <div class="col l12">
    <%= f.label :description %>
    <%= f.text_area :description, class: "materialize-textarea" %>
  </div>
  <button class="btn">
    <%= f.submit %>
  </button>
<% end %>
</div>
</div>